<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MehndiInterior" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:fontFamily">serif</item>
        <item name="fontFamily">serif</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    
    <style name="myButton">
        <item name="android:foreground">@drawable/foreground_click_effect</item>
        <item name="android:backgroundTint">@color/ten1</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/shape_corner20dp_stroke_black</item>
    </style>
    <style name="myBackround">
        <item name="android:background">@color/white</item>
        <item name="android:backgroundTint">@color/white</item>
    </style>
    <style name="myText">
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="myTextReverse">
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="myBackround2">
        <item name="android:background">@color/sixty5</item>
        <item name="android:backgroundTint">@color/sixty5</item>
    </style>
    <style name="myEdittext">
        <item name="android:textColorHint">@color/thirty5</item>
        <item name="android:textColor">@color/black</item>
        <item name="backgroundTint">@color/white</item>
        <item name="android:background">@drawable/shape_corner20dp_stroke_black</item>
    </style>
